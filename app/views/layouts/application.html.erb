<!DOCTYPE html>
<html>
  <head>
    <title>EagleTracks</title>
    <%= stylesheet_link_tag    "application", media: "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
    <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
  </head>
  <body>
    <header class="navbar navbar-fluid-top">
      <div class="navbar-inner">
        <div class="container">
          <%= link_to "Eagle Tracks", '#', id: "logo" %>
          <nav>
            <ul class="nav pull-right">
              <li><%= link_to "Users",    '/users' %></li>
              <li><%= link_to "User Types",    '/user_types' %></li>
              <li><%= link_to "Requirements", '/requirements' %></li>
              <li><%= link_to "Completed Requirements", '/completed_requirements' %></li>
              <li><%= link_to "Awards", '/awards' %></li>
              <li><%= link_to "Awards Earned", '/earned_awards' %></li>
            </ul>
          </nav>
        </div>
      </div>
    </header>
    <div class="container">
      <%= yield %>
    </div>
  </body>
</html>

